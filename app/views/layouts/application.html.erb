    <div class="leftbar">
      <div id="category">
        <h3>Categories</h3>
        <li>category 1</li>
        <li>category 2</li>
      </div>
    </div>

    <div class="mainarea">
      <%= yield %>
    </div>
    <div id="blank"></div>
  </div>

</body>

</html>
